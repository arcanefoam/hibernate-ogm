@GenModel(modelDirectory="/hibernate-ogm-neoemf/src/test/java-gen",
    modelPluginID="",
    modelName="OgmTest",
    modelPluginClass="",
    rootExtendsInterface="fr.inria.atlanmod.neoemf.core.PersistentEObject",
    rootExtendsClass="fr.inria.atlanmod.neoemf.core.DefaultPersistentEObject",
    reflectiveDelegation="true",
    importerID="org.eclipse.emf.importer.ecore",
    bundleManifest="false",
    featureDelegation="Reflective",
    complianceLevel="8.0"
